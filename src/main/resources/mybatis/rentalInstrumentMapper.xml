<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="se.kth.iv1351.dao.RentalInstrumentMapper">
    <select id="selectRentalInstrument" parameterType="java.lang.Integer" resultType="se.kth.iv1351.model.RentalInstrumentData">
        SELECT * FROM BOOKINGS WHERE id= #{rentalid};
    </select>
    <insert id="insertRentalInstrument" parameterType="se.kth.iv1351.openapi.model.RentalInstrument">
        INSERT INTO BOOKINGS (id, description, serial_number, instrument_type_id)
        VALUES (#{id}, #{description} #{serialNumber}, #{instrumentTypeId});
    </insert>
</mapper>